                        emit onePingOnlyVersaillies();
                        qWarning() << "Found a negative resistance value.  Throwing out measurement set and taking new ones.";
                    }
                }
                else
                {
//                    qWarning() << "Break In one ping only sent.";
//                    onePingTimer.start(200);
                    emit onePingOnlyVersaillies();
