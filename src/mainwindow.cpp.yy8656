void MainWindow::busyTimeout()
{
    static int num(0);
    switch(num)
    {
    case 0:
        ui->textBusy->setText("--");
        num = 1;
        break;
    case 1:
        ui->textBusy->setText("\\");
        num = 2;
        break;
    case 2:
        ui->textBusy->setText("|");
        num = 3;
        break;
    case 3:
        ui->textBusy->setText("/");
        num = 0;
        break;
    }
}

